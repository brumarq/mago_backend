{
  "ConnectionStrings": {
    "DevicesDb": "Server=${DEVICE_DB_SERVER},${DEVICE_DB_PORT};Initial Catalog=${DEVICE_DB_CATALOG};Persist Security Info=False;User ID=${DEVICE_DB_USERID};Password=${DEVICE_DB_PASSWORD};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;",
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth0": {
    "Domain": "${AUTH0_DOMAIN}",
    "Audience": "${AUTH0_AUDIENCE}",
    "ClientId": "${AUTH0_CLIENT_ID}",
    "ClientSecret": "${AUTH0_CLIENT_SECRET}"
  }
}